            <ixy>0.0</ixy>
            <iyy>0.33552910</iyy>
            <ixz>0.0</ixz>
            <iyz>0.0</iyz>
            <izz>0.33552910</izz>
          </inertia>
        </inertial>

        <visual name="visual">
          <pose>0.04 0.0 0.0 0 0 0</pose>
          <geometry>
            <mesh>
              <uri>model://prius_hybrid/meshes/Hybrid.obj</uri>
              <scale>0.01 0.01 0.01</scale>
